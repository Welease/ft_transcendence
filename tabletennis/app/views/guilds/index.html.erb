<h1>Guilds</h1>
<h1>Here we add table with all guilds</h1>
<h1>
    <h2>List: guilds </h2><br>
    <% @guilds.each do |guild| %>
        <tr>
            <td><%= guild.name %></td>
            <td><%= guild.anagram %></td>
            <td><%= guild.rating %></td>
            <td><%= guild.nubmer_of_users %></td>
            <td><%= guild.photo %></td>
        </tr>
    <% end %>


